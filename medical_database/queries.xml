PREFIX : <http://example.org/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX doco: <http://purl.org/spar/doco/>
PREFIX dbr: <http://dbpedia.org/resource/>
PREFIX po: <http://www.essepuntato.it/2008/12/pattern#>
PREFIX Fabio: <http://purl.org/spar/fabio/>
PREFIX dbr: <http://dbpedia.org/resource/>
<!-- Resposta 1 -->
SELECT ?url ?header WHERE {
?t1  rdf:type    rdf:Statement ;
     rdf:subject    dbr:Anemia;
     rdf:predicate  :risk_factor_for ;
     rdf:object     :Pregnancy.

?t2  rdf:type    rdf:Statement ;
     rdf:subject    :Pregnancy;
     rdf:predicate  :treatment ;
     rdf:object     dbr:Multivitamin.
  
  
?p3 po:contains ?t1 , ?t2.
?p2 po:contains ?p3.
?p2 po:containsAsHeader ?header.
?p1 po:contains ?p2.
?doc po:contains ?p1.
:doc1   Fabio:hasPortrayal  :arq1 .
:arq1   Fabio:hasURL  ?url .
} 

